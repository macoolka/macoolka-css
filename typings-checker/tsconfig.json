{
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "target": "es5",
    "lib": [
      "dom",
      "es2016",
      "es2017.object",
      "es5",
      "es6",
      "es2015.core",
      "es2015.collection",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "esnext.asynciterable"
    ],
  }
}
