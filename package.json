{
  "name": "mocoolka-css",
  "version": "0.3.17",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "types": "lib/index.d.ts",
  "author": "fastspeeed@gmail.com",
  "scripts": {
    "reinstall": "rm -rf node_modules/ && yarn install",
    "clean": "rm -rf lib/ es5-module/ jsnext/",
    "lint": "tslint -p './tsconfig.json'",
    "lint:fix": "tslint -p ./tsconfig.json --fix ",
    "test": "jest",
    "test:watch": "jest --watch ./src",
    "build": "rm -rf lib/  && tsc",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "prepublishOnly": "npm run clean && npm run reinstall && npm run lint && npm run build && npm run test",
    "coverage": "rimraf coverage && jest --coverage",
    "dtslint": "dtslint dtslint"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^2.2.1",
    "mocoolka-fp": "^0.1.20"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/jest": "^23.3.2",
    "csstype": "^2.5.7",
    "dtslint": "^0.3.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "keywords": [
    "mocoolka",
    "css",
    "functional"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(./src/*|\\.(test|spec))\\.(ts|tsx)$"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mocoolka/mocoolka-css.git"
  }
}
